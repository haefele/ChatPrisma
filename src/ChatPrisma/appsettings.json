{
  "OpenAI": {
    "Model": "gpt-3.5-turbo",
    "ApiKey": "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",
  },
  "NLog": {
    "throwConfigExceptions": true,
    "targets": {
      "async": true,
      "file": {
        "type": "File",
        "fileName": "${specialFolder:folder=ApplicationData}/ChatPrisma/Logs/log.txt",
        "archiveFileName": "${specialfolder:folder=ApplicationData}/ChatPrisma/Logs/log.{#}.txt",
        "archiveEvery": "Day",
        "archiveNumbering": "Date",
        "archiveDateFormat": "yyyy-MM-dd",
        "maxArchiveFiles": 7,
      },
      "console": {
        "type": "Console",
      }
    },
    "rules": [
      {
        "logger": "*",
        "minLevel": "Info",
        "writeTo": "console"
      },
      {
        "logger": "ChatPrisma.*",
        "minLevel": "Trace",
        "writeTo": "console"
      },
      {
        "logger": "*",
        "minLevel": "Debug",
        "writeTo": "file"
      }
    ]
  }
}